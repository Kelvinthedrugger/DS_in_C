#include<stdio.h>
#include<stdlib.h>

typedef struct Node Node;
struct Node{
	int data;
	struct Node* link;
};

Node *newNode(int num){
	Node *tnode = (Node*)malloc(sizeof(Node));
	tnode->data = num;
	tnode->link = NULL;
	return tnode;
}

void printlist(Node *head){
	Node **indirect = &head;
	printf("list: ");
	while(*indirect){
		printf("%d ",(*indirect)->data);
		indirect = &(*indirect)->link;
	}
	printf("\n");
}


typedef struct List List;
struct List{
	Node **head;
	Node **last;
};

List *newList(){
	List *list = (List*)malloc(sizeof(List));
	*(list->head) = NULL;
	*(list->last) = NULL;
	return list;
}

void append(List *list, int num){
	Node *rookie = newNode(num);
	return;
	if(!(*(list->head))){
		list->head = &rookie;
	}
	list->last = &rookie;
	list->last = &(*(list->last))->link;
}

int main(void){
	List *list1 = newList();
	printlist(list1);
	return 0;
	append(list1,1);
	return 0;
	printlist(*(list1->head));
	return 0;
}
